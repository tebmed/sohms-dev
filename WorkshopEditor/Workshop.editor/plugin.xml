<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated workshopmodel -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="Workshop.presentation.WorkshopModelWizardID"
            name="%_UI_WorkshopModelWizard_label"
            class="Workshop.presentation.WorkshopModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/WorkshopModelFile.gif">
         <description>%_UI_WorkshopModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated workshopmodel -->
      <editor
            id="Workshop.presentation.WorkshopEditorID"
            name="%_UI_WorkshopEditor_label"
            icon="icons/full/obj16/WorkshopModelFile.gif"
            extensions="workshop"
            class="Workshop.presentation.WorkshopEditor"
            contributorClass="Workshop.presentation.WorkshopActionBarContributor">
      </editor>
   </extension>

</plugin>
